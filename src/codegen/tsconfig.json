{
	"compilerOptions": {
	  "lib": [
		"dom",
		"dom.iterable",
		"esnext"
	  ],
	  "allowJs": true,
	  "skipLibCheck": true,
	  "strict": false,
	  "noEmit": true,
	  "incremental": true,
	  "esModuleInterop": true,
	  "module": "ESNext",
	  "target": "ES6",
	  "moduleResolution": "Node",
	  "resolveJsonModule": true,
	  "isolatedModules": true,
	  "jsx": "preserve",
	},
	"include": [
	  "next-env.d.ts",
	  "**/*.ts",
	  "**/*.tsx"
  , "index-content.ts" , "index-content.ts" ],
	"exclude": [
	  "node_modules"
	]
  }
